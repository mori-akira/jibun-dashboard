{
    "editor.formatOnSave": true,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "cSpell.words": [
        "chartjs",
        "datatable",
        "datepicker",
        "dbaeumer",
        "devcontainers",
        "donjayamanne",
        "eamodio",
        "esbenp",
        "githistory",
        "Gruntfuggly",
        "iconify",
        "jibun",
        "mhutchie",
        "mosapride",
        "Nuxt",
        "onwarn",
        "openapitools",
        "registerables",
        "tabler",
        "uctakeoff",
        "unhead",
        "vuepic",
        "zenkaku"
    ],
    "explorer.fileNesting.enabled": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[css]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    }
}
