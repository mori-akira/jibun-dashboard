{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {
      "v3.0": {
        "inputSpec": "openapi.yaml",
        "generatorName": "typescript-axios",
        "output": "../front/api/client",
        "additionalProperties": {
          "supportsES6": true,
          "withInterfaces": true
        },
        "globalProperty": {
          "apiDocs": false,
          "modelDocs": false
        }
      }
    }
  }
}
